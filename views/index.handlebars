<h1>Burger Time!</h1>

<h2>Burgers that have not been devoured</h2>
    <ul>
        {{#each burgers}}
            {{#unless devoured}}
                <li>{{burger_name}}</li>
            {{/unless}}
        {{/each}}
    </ul>

<h2>Burgers that have been devoured</h2>
    <ul>
        {{#each burgers}}
            {{#if devoured}}
                <li>{{burger_name}}</li>
            {{/if}}
        {{/each}}
    </ul>